{
    "csharp.format.enable": true,
    "editor.formatOnSave": true,
    "editor.formatOnSaveTimeout": 5000,
    "[csharp]": {
      "editor.tabSize": 4
    },
    "editor.insertSpaces": true,
    "files.eol": "\n",
    "files.exclude": {
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/CVS": true,
      "**/.DS_Store": true
    },
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,
    "cSpell.words": [
        "verticalmade"
    ]
}